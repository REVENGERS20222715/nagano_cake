<table class='table'>
  <thead>
    <tr>
      <th>ジャンル検索</th>
    </tr>
  </thead>
  <tbody>
    <% genres.each do |genre| %>
      <tr>
　　　　<!--                              カラム名　genre(変数)のid-->
        <td><%= link_to public_items_path(genre_id: genre.id, genre_name: genre.name) do %>
          <%= genre.name %><% end %>
        </td>


      </tr>
    <% end %>
  </tbody>
</table>
