<div class="container">
  <h5>ジャンル編集</h5>
　　<%= form_with model: @genre, url: admin_genre_path do |f| %>
      <div class="form-group">
        <%= f.label :"ジャンル名　" %>
        <%= f.text_field :name %>
      </div>
      <div class="form-group">
        <input type="submit" name="commit" value="Update Genre" class="btn btn-success" data-disable-with="Update Genre">
      </div>
  <% end %>
</div>